{
  "name": "@nest-mind/mcp-server",
  "version": "0.2.5",
  "description": "Complete NestJS implementation of the Model Context Protocol (MCP) server with all core types and utilities",
  "author": "nest-mind",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "mcp",
    "model-context-protocol",
    "ai",
    "llm",
    "server",
    "websocket",
    "sse",
    "http",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:nest-mind/mcp.git",
    "directory": "packages/server"
  },
  "homepage": "https://github.com/nest-mind/mcp/tree/main/packages/server",
  "bugs": {
    "url": "https://github.com/nest-mind/mcp/issues"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "jest",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build && pnpm run test && pnpm run validate:package",
    "validate:package": "node -e \"const pkg = require('./package.json'); const fs = require('fs'); if (!fs.existsSync(pkg.main)) throw new Error('Main file not found: ' + pkg.main); if (!fs.existsSync(pkg.types)) throw new Error('Types file not found: ' + pkg.types); console.log('âœ… Package validation passed');\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.4",
    "@nest-mind/mcp-core": "^0.2.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/websockets": "^11.1.0",
    "@types/node-fetch": "^2.6.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-jsonschema": "^5.0.2",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "path-to-regexp": "^8.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@nestjs-mcp/eslint-config": "workspace:*",
    "@nestjs-mcp/jest-config": "workspace:*",
    "@nestjs-mcp/typescript-config": "workspace:*",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/event-emitter": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "@nestjs/schedule": "^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0 || ^6.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  }
}
