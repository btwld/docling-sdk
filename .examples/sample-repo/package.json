{
  "name": "nest-mind-mcp",
  "version": "0.1.0",
  "description": "NestJS implementation of the Model Context Protocol (MCP) ecosystem",
  "author": "conceptatech",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:nest-mind/mcp.git"
  },
  "keywords": [
    "nestjs",
    "mcp",
    "model-context-protocol",
    "ai",
    "llm",
    "monorepo"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "test:watch": "turbo run test:watch",
    "test:performance": "turbo run test:performance",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "clean": "turbo run clean",
    "check-types": "turbo run check-types",
    "deps:check": "syncpack lint",
    "deps:fix": "syncpack fix-mismatches",
    "deps:update": "syncpack update",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.27.9",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "syncpack": "^13.0.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^2.5.4",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
